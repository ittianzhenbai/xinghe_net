*
  margin: 0; 
  padding: 0;
  box-sizing: border-box;
  user-select: none;
  list-style: none;

html,body
  height: 100%;
  width: 100%;
  margin 0 
  padding 0
  font-family: "Microsoft YaHei"

ul, ol, li 
  list-style: none;
a 
  text-decoration: none;
  color: #333333;
  -webkit-backface-visibility: hidden
a:hover 
    text-decoration: none;
input 
  border none 
  outline: none
header, footer, nav, section, article 
    display: block
.clear:after 
    content: '';
    width: 0;
    height: 0;
    display: block;
    clear: both;
.hide 
    display: none;
.show 
    display: block;
.clearfix:after
  display:table
  content:''
  height:0
  overflow:hidden
  clear:both
.clearfix:before
  display:table
  content:''
*{
    box-sizing: border-box
    scrollbar-arrow-color rgba(3, 5, 57, 1)
    scrollbar-base-color hsla(0, 0%, 53%, 0.4)
    scrollbar-track-color rgba(3, 5, 57, 1)
    scrollbar-shadow-color hsla(0, 0%, 53%, 0.1)
    &::-webkit-scrollbar {
        width: 6px;
        height: 6px;
        background: transparent;
    }
    &::-webkit-scrollbar-thumb {
        background: transparent;
        border-radius: 4px;
    }
    &:hover::-webkit-scrollbar-thumb {
        background: hsla(0, 0%, 53%, 0.4);
    }
    &:hover::-webkit-scrollbar-track {
        background: hsla(0, 0%, 53%, 0.1);
    }
}
.swiper-pagination-bullet
    background-color #FFF!important
.swiper-pagination-bullet-active
    background-color #FFF!important
// 分别是修改分页组件的中部颜色 右边颜色 左边颜色
.el-pager li
    background-color #EAEAEA !important
    color #999999
    font-family MicrosoftYaHei
    font-weight Regular
    font-size 0.5rem
    margin 0 1.5px !important
.el-pager li:hover
    background-color #2993E8 !important
    color #FFFFFF !important
.el-pagination .btn-next
    color #999999
    background-color #EAEAEA !important
.el-pager li.active
    color #FFFFFF
    background-color #1A649F !important
