//主页动画
@keyframes myfirst
{
    0% {
        background rgba(255,0,0,0)
        opacity 0
    }
    25%{
        background rgba(41,50,57,.2)
    }
    50%{
        background rgba(41,50,57,.3)
    }
    75%{
        background rgba(41,50,57,.5)
    }
    100% {
        visibility visible
        background rgba(41,50,57,.6)
        opacity 1
    }
}
//修改手机端子菜单下拉导航菜单样式
@media screen and (max-width:768px)
    .el-menu--inline
        .el-menu-item 
          color red
          background-color #409EFF !important
    .el-menu-item //修改子菜单的背景颜色
        // background-color #409EFF !important
        padding-left 20px!important
        font-size 1.5rem !important
        border-bottom 1px solid #FFFFFF
    .el-aside //修改子菜单的字体颜色
        color #FFFFFF
        font-size 3rem !important
    .el-menu-item:hover //鼠标悬浮时候，子菜单的样式
        outline 0 !important
        color #409EFF !important
    .el-menu-item.is-active //鼠标点击时候，子菜单的样式
        color #409EFF !important
        background #333 !important
    .el-submenu__title:focus .el-submenu__title:hover//鼠标悬浮时，主菜单的样式：
        outline 0 !important
        color yellow !important
        background none !important
   .el-submenu__title //二级菜单标题
        font-size 1rem !important
        border-bottom 1px solid #FFFFFF
        color #FFFFFF !important
        @media screen and (max-width:768px)
            font-size 1.5rem !important
            font-family MicrosoftYaHei !important
            font-weight Regular !important
    .el-submenu__icon-arrow,.el-icon-arrow-down//修改二级菜单 下拉按钮颜色
        color #FFFFFF !important
        font-weight 1000 !important
        @media screen and (max-width:768px)
            font-size 1.8rem !important
    .child_nav//手机端各级子页面菜单
        .el-submenu__title
            color #1A649F !important
            font-size 1.8rem !important
            background #F2F2F2 !important
        .el-submenu__icon-arrow,.el-icon-arrow-down
            color #223D6B !important
        .el-menu-item 
            font-size 1.4rem !important
    